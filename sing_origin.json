{
  "dns": {
    "servers": [
      {
        "tag": "cloudflare",
        "address": "1.1.1.1",
        "strategy": "prefer_ipv6"
      }
    ]
  },
  "outbounds": [
    {
      "tag": "direct",   // 直接连接
      "type": "direct",
      "domain_strategy": "prefer_ipv4"
    },
    {
      "tag": "block",    // 阻止访问
      "type": "block"
    },
    {
      "tag": "socks_proxy",  // SOCKS 代理
      "type": "socks",
      "server": "127.0.0.1",
      "server_port": 40000
    }
  ],
  "route": {
    "rules": [
      {
        "ip_is_private": true,  // 局域网流量拦截
        "outbound": "block"
      },
      {
        "geoip": "cn",
        "outbound": "block"
      },
      {
        "domain_regex": [  // 拦截的域名规则（有需要可以继续添加）
"(api|ps|sv|offnavi|newvector|ulog.imap|newloc)(.map|).(baidu|n.shifen).com",
"(.+.|^)(360|so).(cn|com|net)",
"(Subject|HELO|SMTP)",
"(torrent|.torrent|peer_id=|info_hash|get_peers|find_node|BitTorrent|announce_peer|announce.php?passkey=|magnet:|xunlei|sandai|Thunder|XLLiveUD|bt_key)",
"(^.@)(guerrillamail|guerrillamailblock|sharklasers|grr|pokemail|spam4|bccto|chacuo|027168).(info|biz|com|de|net|org|me|la)",
"(.?)(xunlei|sandai|Thunder|XLLiveUD)(.)",
"(.*.||)(dafahao|mingjinglive|botanwang|minghui|dongtaiwang|falunaz|epochtimes|ntdtv|falundafa|falungong|wujieliulan|zhengjian).(org|com|net)",
"(.*.||)(guanjia.qq.com|qqpcmgr|QQPCMGR)",
"(.*.||)(rising|kingsoft|duba|xindubawukong|jinshanduba).(com|net|org)",
"(.*.||)(netvigator|torproject).(com|cn|net|org)",
"(..||)(visa|mycard|gash|beanfun|bank).",
"(.*.||)(gov|12377|12315|talk.news.pts.org|creaders|zhuichaguoji|efcc.org|cyberpolice|aboluowang|tuidang|epochtimes|zhengjian|110.qq|mingjingnews|inmediahk|xinsheng|breakgfw|chengmingmag|jinpianwang|qi-gong|mhradio|edoors|renminbao|soundofhope|xizang-zhiye|bannedbook|ntdtv|12321|secretchina|dajiyuan|boxun|chinadigitaltimes|dwnews|huaglad|oneplusnews|epochweekly|cn.rfi).(cn|com|org|net|club|net|fr|tw|hk|eu|info|me)",
"(.*.||)(miaozhen|cnzz|talkingdata|umeng).(cn|com)",
"(.*.||)(mycard).(com|tw)",
"(.*.||)(gash).(com|tw)",
"(.bank.)",
"(.*.||)(pincong).(rocks)",
"(.*.||)(taobao).(com|cn)",
"(.*.||)(laomoe|jiyou|ssss|lolicp|vv1234|0z|4321q|868123|ksweb|mm126|ping0).(com|cloud|fun|cn|gs|xyz|cc)",
"(flows|miaoko).(pages).(dev)"

        ],
        "outbound": "block"
      },
      {
        "domain_regex": [  // 走 SOCKS 代理的域名（有需要可以继续添加）
          "openai.com", // OpenAI
          "oaiusercontent.com", 
          "google", // Google
          "gstatic.com", 
          "gvt1.com", 
          "ggpht.com", 
          "googlevideo.com", 
          "ytimg.com", // YouTube
          "youtube.com", 
          "instagram.com", // 社交媒体
          "cdninstagram.com", 
          "facebook.com", 
          "fbcdn.net", 
          "fbsbx.com", 
          "instagramad.com", 
          "akamaihd.net", 
          "stripe.com", // 支付
          "amazon",  // 亚马逊
          "cloudflare.com", // 云服务
          "cloudns.net",
          "intercom.io", // 客服
          "pypi.org", // 开发工具
          "npmjs.com", 
          "sentry.io", 
          "diwodiwo.xyz", // bika
          "picacomic.com", // bika
          "hanime1.me", 
          "browserscan.net",  // IP 检测
          "test-ipv6", 
          "fofa.info", 
          "ipleak.net" 
        ],
        "outbound": "socks_proxy"
      },
      {
        "outbound": "direct",  // 其他流量直连
        "network": ["udp", "tcp"]
      }
    ]
  },
  "experimental": {
    "cache_file": { "enabled": true }  // 启用缓存
  }
}
